{
  "name": "fb-bot",
  "version": "1.0.0",
  "description": "this is template repo for nodejs typescript",
  "main": "dist/index.js",
  "scripts": {
    "tsc": "tsc",
    "tsc-w": "tsc -w",
    "start": "node dist/index.js",
    "build": "esbuild --bundle src/index.ts --outdir=dist --platform=node --target=ESNext --minify",
    "dev": "env-cmd -f .env nodemon --watch src src/index.ts",
    "test": "npx ts-node src/index.ts"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "express-form-data": "^2.0.23",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.4",
    "puppeteer": "^22.5.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rotating-file-stream": "^3.2.1",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-form-data": "^2.0.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
